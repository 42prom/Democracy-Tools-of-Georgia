{
  "name": "dtg-backend",
  "version": "0.1.0",
  "description": "DTG Backend API - Phase 0",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "migrate": "cross-env tsx src/db/migrate.ts",
    "migrate:status": "cross-env tsx src/db/migrate.ts status",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "lint": "echo \"Linting disabled temporarily for CI stability\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:reset": "npm run migrate && npx tsx src/scripts/reset_admin_password.ts",
    "db:clear-polls": "tsx scripts/clear_polls_votes.ts",
    "validate:contracts": "tsx scripts/validate-openapi.ts"
  },
  "keywords": [
    "voting",
    "privacy",
    "nfc",
    "georgia"
  ],
  "author": "Mikheili Nakeuri",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.4",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "crypto": "^1.0.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "pino-http": "^11.0.0",
    "prom-client": "^15.1.3",
    "redis": "^5.10.0",
    "snarkjs": "^0.7.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.3.0",
    "@types/pg": "^8.16.0",
    "@types/pino-http": "^5.8.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "audit-ci": "^7.1.0",
    "cross-env": "^10.1.0",
    "eslint": "8.57.0",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "solc": "^0.5.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "ajv": "8.18.0",
    "minimatch": "10.2.3",
    "fast-xml-parser": "4.3.8",
    "rollup": "4.59.0",
    "jsonpath": "1.1.1",
    "yargs-parser": "21.1.1",
    "bfj": "8.0.0"
  }
}
